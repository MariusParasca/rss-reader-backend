{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier", "plugin:import/errors", "plugin:import/warnings"],
  "plugins": ["prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": ["error"]
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  }
}
